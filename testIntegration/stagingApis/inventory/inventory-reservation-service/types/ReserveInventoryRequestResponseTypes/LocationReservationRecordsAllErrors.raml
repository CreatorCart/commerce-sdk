#%RAML 1.0 DataType

#LocationReservationRecordsAllErrors
description: Location level line items for reservation responses.
properties:
  id:
    type: !include ../GlobalTypes/LocationId.raml
    description: The location ID associated with these reservations
    required: true
    example: warehouse5
  errors:
    type: array
    description: Errors that were encountered while trying to place this location reservation
    items: !include ReservationErrorLines.raml